<%- include("partials/header");-%>
<div class="slider-down"></div>
<div class="container extra-pad normal-font">

<h1 class="move-up-on-start color-green">Candidates for <%= type%></h1>

<%data.forEach(function(element){%>


    <% let str1 = "/candidates/"+type+"/"+element.name+"/";%>
    <h4> <a class="no-format-candidates" href="<%-str1%>"><%=element.name%></a></h4>

<%});%>



</div>
